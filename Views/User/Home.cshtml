
@using SignUp_SignIn.Models

<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/css/materialize.min.css">

    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/js/materialize.min.js"></script>

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <style>
	import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@100&display=swap');

    import url('https://fonts.googleapis.com/css2?family=Sacramento&display=swap');
	*{

	}
header{
  background:url(/images/guitar1.png);
  background-size:cover;
  position:center;
  min-height:1300px;
  position:static;

}
@@media screen and (max-width:500px){
header{
  min-height:500px;
}
}
h3{
	font-family:sacramento;

	}

.card {
  transition: 0.5s;
  padding:10px 10px 10px 10px 10px;

}

.card:hover {
  transform: scale(1.05);

  opacity:0.7;
}
.card:hover a{
  transform: scale(1.05);
  background:black;

}
.row .col{
	padding:20px;
	width:100%;

}
#music{
	width:79%;
}
#x1 {

	background:black;
	text-align:center;


}
input,select,textarea{
	color:white;

}

#mobile-nav {


 padding:50px 50px 50px 50px;
 text-align:center;

}
#mobile-nav li{
 text-align:center;
 font-size:30px;


}
#mobile-nav ul p{
 text-align:center;


}
#logo {

font-size:30px;
font-family:sacramento;
}
#mobile-nav li a:hover{
	opacity:0.9;
}

#desk li a:hover
 {
  transition: 0.5s;

  color:brown;

 }
 #x2{
 	text-decoration:none;
 	color:grey;
 }




    </style>



</head>
<body>

    <header>
        <nav class="nav-wrapper black ">
            <div class="container">
                <a href="#" class="brand-logo" id="logo">hub de música</a>

                <a href="#" class="sidenav-trigger" data-target="mobile-nav">
                    <i class="material-icons">menu</i>
                </a>

                <ul class="right hide-on-med-and-down" id="desk">

                    <li><a href="#">Home</a></li>
                    <li><a href="#x1">Contact</a></li>
                    <li><a href="#music">Music</a></li>
                    <li><a href="#about">About</a></li>


                </ul>



                <ul class="sidenav black" id="mobile-nav">

                    <center> <font color="white"><font face="sacramento"><font size="50"><p class="center-align">hub de música</p></font></font></font>


                    <li><a href="#" class="btn teal darken-1 waves-effect waves-light">Home</a></li>
                    <li><a href="#x1" class="btn teal darken-1 waves-effect waves-light">Contact</a></li>

                    <li><a href="#music" class="btn teal darken-1 waves-effect waves-light">Music</a></li>
                    <li><a href="#about" class="btn teal darken-1 waves-effect waves-light">About</a></li>
                    <li><img src="/images/imgx.png" style="width:30%; transform:rotate(20deg);"></li>




                </ul>
            </div>
        </nav>
    </header>

    <div class="container service scrollspy" id="music">
        <h4 class="teal-text text-darken-1 center-align">Comic menu</h4>
        <p class="center-align">Find your favourite over here.</p>
        <div class="row">
            <div class="col s12 md6 L4  center-align">
                <div class="card">
                    <div class="card-image">
                        <img src="/images/yy7.png" alt="">
                    </div>
                    <div class="card-content">
                        <span class="card-title">Guitar whales</span>

                        <p>Here you go!,A couple of aesthetic and soothing guitar tunes.</p>
                    </div>
                    <div class="card-action">
                        <a asp-controller="Tunes"
                           asp-action="Guitar" class="btn waves-effect waves-light">
                            View More
                        </a>
                    </div>
                </div>
            </div>
            <div class="col s12 md6 L4 center-align">
                <div class="card">
                    <div class="card-image">
                        <img src="/images/yy7.png" alt="">
                    </div>
                    <div class="card-content">
                        <span class="card-title">Ukulele whales</span>

                        <p>Here you go!,A couple of aesthetic and soothing ukulele tunes.</p>
                    </div>
                    <div class="card-action">
                        <a asp-controller="Tunes"
                           asp-action="Ukulele" class="btn waves-effect waves-light">
                            View More
                        </a>
                    </div>
                </div>
            </div>
            <div class="col s12 md6 L4 center-align">
                <div class="card">
                    <div class="card-image">
                        <img src="/images/yy7.png" alt="">
                    </div>
                    <div class="card-content">
                        <span class="card-title">Anonymous whales</span>

                        <p>Here you go!,A couple of spooky and anonymous tunes.</p>
                    </div>
                    <div class="card-action">
                        <a asp-controller="Tunes"
                           asp-action="Spooky" class="btn waves-effect waves-light">
                            View More
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container scrollspy" id="x1">

        <div class="row ">
            <div class="col s12 md6 l5">
                <h2 class="teal-text text-darken-1">Contact us</h2>
                <p><font face="montserrat"><font color="white">Hey, there! Contact us via email, throw a message if required. Write your comments about our site. Cover up the input fields, enter your suggestions and submit them.</font></font></p>
            </div>
            <div class="col s12 md6 l5 offset-12">
                <form action="/User/Contact" method="post">
                    <div class="input-field">
                        <i class="material-icons teal-text text-darken-1 prefix">email</i>
                        <input type="email" id="email">
                        <label for="email">Email</label>
                    </div>
                    <div class="input-field">
                        <i class="material-icons teal-text text-darken-1 prefix">message</i>

                        <textarea id="message" class="materialize-textarea"></textarea>
                        <label for="message">Write your message</label>
                    </div>
                    <div class="input-field">

                        <input type="text" id="date" class="datepicker">
                        <label for="date">Select a date:)</label>
                    </div>
                    <div class="input-field center">

                        <button class="btn waves-effect waves-light">submit</button>

                    </div>


                </form>

            </div>


        </div>
    </div>
    <div class="center">@ViewBag.data</div>
    <footer class=" page-footer grey darken-3">
        <div class="container center-align scrollspy" id="about">


            <font color="grey">
                <h3>About us</h3>
                <p>We offer a couple of instrumental tunes including guitar and ukulele, Moreover find some dark tunes , too , on this site . You can either download or listen to these amazing tunes on our site. PS (this website would get innovated by time)</p>






        </div>
        <div class="footer-copyright grey darken-4">
            <div class="container center-align">&copy; 2021 hub de música</div>

        </div>


    </footer>



    <script>
		$(document).ready(function(){
 		$('.sidenav').sidenav();
 		$('.datepicker').datepicker({

 		});
 		$('.scrollspy').scrollSpy();


 	});

    </script>
</body>
